x=int(input("what is x  ? "))
y=int(input("what is y  ? "))
z=input("which operations do u want to perform  + or - or * or / " )
if z == '+':
    print(f"{x+y}")
elif z == '-':
    print(f"{x-y}")
elif z == '*':
    print(f"{x*y}")
elif z == '/':
    print(f"{x/y}")
else:
    print("invalid operation")
